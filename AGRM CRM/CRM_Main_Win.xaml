                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="100"/>
                                <RowDefinition Height="607*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Должности" Grid.Row="0" Style="{DynamicResource Name_Subsection}"/>
                            <Button Click="Click_Create" Style="{DynamicResource CreateButton}"/>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="59*"/>
                                    <ColumnDefinition Width="285*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Column="1" Style="{DynamicResource StackPanelStyle}">
                                    <TextBox x:Name="PositionsTxt" TextChanged="Seartch_TextChanged" Style="{DynamicResource SearchTextBox}"/>
                                    <Button Content="✖" Click="Delete_Click" Style="{DynamicResource SearchButSet}"/>
                                    <Button Content="⟳" Click="Restart_Click" Style="{DynamicResource SearchButSet}" Cursor="Hand"/>
                                </StackPanel>
                                <TextBlock Name="PositionsHint" Grid.Column="1" Style="{DynamicResource SearchTextBlock}"/>
                                <ComboBox Name="FilterPositions" Style="{DynamicResource ComboBoxStyle}" Template="{DynamicResource ComboBoxTemplate1}" IsEnabled="False" Grid.Column="1" Margin="0 20 17 0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="307" DropDownClosed="Filter_DropDownClosed">
                                    <ComboBoxItem Content="ID" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Должность" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Заработная Плата" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Не выбрано" Style="{DynamicResource ComboBoxItemStyle}" Visibility="Collapsed"/>
                                </ComboBox>
                                <CheckBox Name="CheckPositions" Grid.Column="1" Style="{DynamicResource CheckBoxStyle}" HorizontalAlignment="Right" Margin="0,27,330,0" VerticalAlignment="Top" Click="CheckBox_Click"/>
                            </Grid>
                            <DataGrid Name="DataPositions" Grid.Row="1" RowStyle="{StaticResource DGR}" Style="{DynamicResource DataGridStyle}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" Margin="5 0 15 15"/>
                        </Grid>
                    </TabItem>

                    <TabItem Name="Payments_Subsection" Header="Выплата ЗП" PreviewMouseLeftButtonUp="TabItem_PreviewMouseLeftButtonUp" Style="{DynamicResource TabItem_Subsection}" Cursor="Hand">
                        <Grid>
                            <Grid.RowDefinitions>
                                <RowDefinition Height="100"/>
                                <RowDefinition Height="607*"/>
                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition/>
                            </Grid.ColumnDefinitions>
                            <TextBlock Text="Выплата ЗП" Grid.Row="0" Style="{DynamicResource Name_Subsection}"/>
                            <Button Click="Click_Create" Style="{DynamicResource CreateButton}"/>
                            <Grid Grid.Row="0">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="59*"/>
                                    <ColumnDefinition Width="285*"/>
                                </Grid.ColumnDefinitions>

                                <StackPanel Grid.Column="1" Style="{DynamicResource StackPanelStyle}">
                                    <TextBox x:Name="PaymentsTxt" TextChanged="Seartch_TextChanged" Style="{DynamicResource SearchTextBox}"/>
                                    <Button Content="✖" Click="Delete_Click" Style="{DynamicResource SearchButSet}"/>
                                    <Button Content="⟳" Click="Restart_Click" Style="{DynamicResource SearchButSet}" Cursor="Hand"/>
                                </StackPanel>
                                <TextBlock Name="PaymentsHint" Grid.Column="1" Style="{DynamicResource SearchTextBlock}"/>
                                <ComboBox Name="FilterPayments" Style="{DynamicResource ComboBoxStyle}" Template="{DynamicResource ComboBoxTemplate1}" IsEnabled="False" Grid.Column="1" Margin="0 20 17 0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="307" DropDownClosed="Filter_DropDownClosed">
                                    <ComboBoxItem Content="ID" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Ответственный" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Дата" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Количество" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Общая Сумма" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Не выбрано" Style="{DynamicResource ComboBoxItemStyle}" Visibility="Collapsed"/>
                                </ComboBox>
                                <CheckBox Name="CheckPayments" Grid.Column="1" Style="{DynamicResource CheckBoxStyle}" HorizontalAlignment="Right" Margin="0,27,330,0" VerticalAlignment="Top" Click="CheckBox_Click"/>
                            </Grid>
                            <DataGrid Name="DataPayments" Grid.Row="1" RowStyle="{StaticResource DGR}" Style="{DynamicResource DataGridStyle}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" Margin="5 0 15 15"/>
                        </Grid>
                    </TabItem>

                </TabControl>
            </TabItem>

            <TabItem Name="WareHouses" Header="Склады и наличее" Style="{DynamicResource TabItem_Main}" Cursor="Hand" PreviewMouseLeftButtonUp="TabItem_PreviewMouseLeftButtonUp">
                <Grid Margin="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="607*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Border Style="{DynamicResource BorderBackground}"/>
                    <Border Grid.Row="1" Style="{DynamicResource BorderBackground}"/>
                    <TextBlock Text="Склады и наличее" Grid.Row="0" Style="{DynamicResource Name_Subsection}"/>
                    <Button Click="Click_Create" Style="{DynamicResource CreateButton}"/>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="59*"/>
                            <ColumnDefinition Width="285*"/>
                        </Grid.ColumnDefinitions>
                        <StackPanel Grid.Column="1" Style="{DynamicResource StackPanelStyle}">
                            <TextBox x:Name="WareHousesTxt" TextChanged="Seartch_TextChanged" Style="{DynamicResource SearchTextBox}"/>
                            <Button Content="✖" Click="Delete_Click" Style="{DynamicResource SearchButSet}"/>
                            <Button Content="⟳" Click="Restart_Click" Style="{DynamicResource SearchButSet}" Cursor="Hand"/>
                        </StackPanel>
                        <TextBlock Name="WareHousesHint" Grid.Column="1" Style="{DynamicResource SearchTextBlock}"/>
                        <ComboBox Name="FilterWareHouses" Style="{DynamicResource ComboBoxStyle}" Template="{DynamicResource ComboBoxTemplate1}" IsEnabled="False" Grid.Column="1" Margin="0 20 17 0" VerticalAlignment="Top" HorizontalAlignment="Right" Width="307" DropDownClosed="Filter_DropDownClosed">
                            <ComboBoxItem Content="ID" Style="{DynamicResource ComboBoxItemStyle}"/>
