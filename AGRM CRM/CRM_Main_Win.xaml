                                    <ComboBoxItem Content="Описание" Style="{DynamicResource ComboBoxItemStyle}"/>
                                    <ComboBoxItem Content="Не выбрано" Style="{DynamicResource ComboBoxItemStyle}" Visibility="Collapsed"/>
                                </ComboBox>
                                <CheckBox Name="CheckWare" Grid.Column="1" Style="{DynamicResource CheckBoxStyle}" HorizontalAlignment="Right" Margin="0,27,330,0" VerticalAlignment="Top" Click="CheckBox_Click"/>
                            </Grid>
                            <DataGrid Name="DataWare" Grid.Row="1" RowStyle="{StaticResource DGR}" Style="{DynamicResource DataGridStyle}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" Margin="5 0 15 15"/>
