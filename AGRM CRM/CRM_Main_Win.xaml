                            <ComboBoxItem Content="Всего Товаров" Style="{DynamicResource ComboBoxItemStyle}"/>
                            <ComboBoxItem Content="Не выбрано" Style="{DynamicResource ComboBoxItemStyle}" Visibility="Collapsed"/>
                        </ComboBox>
                        <CheckBox Name="CheckWareHouses" Grid.Column="1" Style="{DynamicResource CheckBoxStyle}" HorizontalAlignment="Right" Margin="0,27,330,0" VerticalAlignment="Top" Click="CheckBox_Click"/>
                    </Grid>
                    <DataGrid Name="DataWareHouses" Grid.Row="1" RowStyle="{StaticResource DGR}" Style="{DynamicResource DataGridStyle}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" Margin="5 0 15 15"/>
                </Grid>
            </TabItem>

            <TabItem Name="Statement" Header="Отчёты" PreviewMouseLeftButtonUp="TabItem_PreviewMouseLeftButtonUp" Style="{DynamicResource TabItem_Main}" Cursor="Hand">
                <Grid Margin="0">
                    <Grid.RowDefinitions>
                        <RowDefinition Height="100"/>
                        <RowDefinition Height="607*"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition/>
                    </Grid.ColumnDefinitions>
                    <Border Style="{DynamicResource BorderBackground}"/>
                    <Border Style="{DynamicResource BorderBackground}" Grid.Row="1"/>
                    <TextBlock Text="Отчёты" Grid.Row="0" Style="{DynamicResource Name_Subsection}"/>
                    <Button Click="Click_Create" Content="Сформировать" Width="170" Style="{DynamicResource CreateButton}"/>
                    <Grid Grid.Row="0">
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="110*"/>
                            <ColumnDefinition Width="377*"/>
                        </Grid.ColumnDefinitions>
                        <DatePicker Name="DateFrom" Style="{DynamicResource DatePickerStyle}" Width="150" Height="30" Grid.Column="1" Margin="0,60,227,10" HorizontalAlignment="Right" CalendarClosed="DatePicker_CalendarClosed"/>
                        <DatePicker Name="DateBefore" Style="{DynamicResource DatePickerStyle}" Width="150" Height="30" Grid.Column="1" Margin="0,60,15,10" HorizontalAlignment="Right" CalendarClosed="DatePicker_CalendarClosed"/>

                        <TextBlock Text="С:" FontSize="20" Width="20" Grid.Column="1" Style="{DynamicResource TextBlockStyle}" Margin="0,60,382,10" HorizontalAlignment="Right"/>
                        <TextBlock Text="По:" FontSize="20" Width="32" Grid.Column="1" Style="{DynamicResource TextBlockStyle}" Margin="0,60,170,10" HorizontalAlignment="Right"/>
                    </Grid>
                    <DataGrid Name="DataStatement" Grid.Row="1" RowStyle="{StaticResource DGR}" Style="{DynamicResource DataGridStyle}" ColumnHeaderStyle="{DynamicResource DataGridColumnHeaderStyle1}" Margin="5 0 15 55"/>
                    <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal" HorizontalAlignment="Left" Height="30" Width="Auto" Grid.Row="1" Margin="15 5 0 15">
                        <TextBlock Text="Оборот:" Style="{DynamicResource Name_Subsection}" Height="30" Width="Auto"/>
                        <TextBox Name="StatementTotalText" Style="{DynamicResource TexBoxStyle}" Height="30" Width="300" Margin="15 0 0 0" IsReadOnly="True"/>
                    </StackPanel>
                </Grid>
            </TabItem>

        </TabControl>
        <Button Margin="65,0,0,10" Grid.Row="1" Style="{DynamicResource ButtonSettings}" Click="SettingsClick" Height="35" Width="34"/>
        <Image Cursor="Hand" HorizontalAlignment="Left" MouseLeftButtonDown="Logout" Margin="10,0,0,10" Grid.Row="1" Width="35" Height="35" VerticalAlignment="Bottom" Source="/Images/icons8-выход-50.png"/>
        <TextBlock TextWrapping="Wrap" x:Name="UserTxt" Cursor="Hand" Margin="10,0,0,45" Grid.Row="1" Height="25" VerticalAlignment="Bottom" HorizontalAlignment="Left" TextAlignment="Left" Width="205" Style="{DynamicResource TextBlockStyle}"/>
        <Border x:Name="Blackout" Background="#303030" BorderThickness="0" Grid.Row="1" Visibility="Hidden" Opacity="0.5"/>
        <Border x:Name="Blackout2" Background="#303030" BorderThickness="0" Visibility="Hidden" Opacity="0.5"/>
    </Grid>
</Window>
