                                    <Button Content="⟳" Click="Restart_Click" Style="{DynamicResource SearchButSet}" Cursor="Hand"/>
                                </StackPanel>
                                <TextBlock Name="WareHint" Grid.Column="1" Style="{DynamicResource SearchTextBlock}"/>
